 

@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1 style="background-color:aquamarine">Create</h1>

<h4>Employee</h4>
<hr />
<div id="myTextArea"></div>
<div class="row" style="background-color:orchid">
    <div class="col-md-4">
        <form >
            <div  class="text-danger"></div>
            <div class="form-group">
                <label   class="control-label"></label>
                <input   class="form-control" id="Name"/>
                <span a  class="text-danger"></span>
            </div>
            <div class="form-group">
                <label   class="control-label"></label>
                <input  class="form-control" id="JobTitle"/>
                <span  class="text-danger"></span>
            </div>
            <div class="form-group">
                <label   class="control-label"></label>
                <input  class="form-control" id="Salary" />
                <span   class="text-danger"></span>
            </div>
            <div class="form-group">
                <label  class="control-label"></label>
                <input  class="form-control" id="HiringDate" />
                <span   class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="button" value="Create" onclick="createEmployee()" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
 

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
